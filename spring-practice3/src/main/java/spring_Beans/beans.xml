<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd"
	default-autowire="byName" 
	default-autowire-candidates="*Writer">


	<bean name="logger" class="springbeans.com.Logger"> </bean>

	<bean name="consoleWriter" class="springbeans.com.ConsoleWriter"></bean>
	
	<bean name="fileWriter" class="springbeans.com.FileWriter"></bean>
	
	<bean name="logWriter" class="springbeans.com.FileWriter"></bean>
</beans>
