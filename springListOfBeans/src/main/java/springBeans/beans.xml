<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">


	<bean name="jungle" class="springListOfBeans.Jungle">
	
		
	<property name="largest" ref="elephant"></property>
		<property name="animals">
			<list>				
				<ref bean="lion" />
				<ref bean="elephant" />
				<ref bean="snake" />
			</list>
		</property>
	</bean>

	<bean name="lion" class="springListOfBeans.Animals">
		<property name="name" value="Ron"></property>
		<property name="type" value="lion"></property>
	</bean>
	<bean name="elephant" class="springListOfBeans.Animals">
		<property name="name" value="Tim"></property>
		<property name="type" value="elephant"></property>
	</bean>

	<bean name="snake" class="springListOfBeans.Animals">
		<property name="name" value="Tim"></property>
		<property name="type" value="snake"></property>
	</bean>
</beans>
